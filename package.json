        "command": "agda.moduleContents",
        "key": "ctrl+o",
        "when": "editorTextFocus && editorLangId == agda && agda.keySequence == 'cc-u'"
      },
      {
        "command": "agda.showConstraints",
        "key": "ctrl+=",
        "when": "editorTextFocus && editorLangId == agda && agda.keySequence == 'cc-u'"
      },
      {
        "command": "agda.showGoals",
        "key": "ctrl+shift+/",
        "when": "editorTextFocus && editorLangId == agda && agda.keySequence == 'cc-u'"
      },
      {
        "command": "agda.solve",
        "key": "ctrl+s",
        "when": "editorTextFocus && editorLangId == agda && agda.keySequence == 'cc-u'"
      }
    ],
