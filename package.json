    "configuration": {
      "title": "Agda",
      "properties": {
        "agda.path": {
          "type": "string",
          "default": "agda",
          "description": "Active Agda binary"
        },
        "agda.additionalPaths": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "Additional Agda binaries to show as \"Agda: Switch Agda Version\" options"
        },
        "agda.extraArgs": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "Extra command-line arguments passed to Agda"
        },
        "agda.backend": {
          "type": "string",
          "enum": [
            "GHC",
            "GHCNoMain",
            "JS",
            "LaTeX",
            "QuickLaTeX",
            "HTML"
          ],
          "default": "",
          "description": "Default backend for Agda compilation"
        },
        "agda.input.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable the Unicode input method (type \\ followed by an abbreviation)"
        },
        "agda.input.leader": {
          "type": "string",
          "default": "\\",
          "description": "The leader character that triggers Unicode input"
        },
        "agda.input.languages": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "agda"
          ],
          "description": "Languages in which Unicode input is enabled"
        },
        "agda.input.customTranslations": {
          "type": "object",
          "default": {},
          "description": "Custom abbreviation overrides (key: abbreviation, value: symbol or array of symbols for Tab-cycling)",
          "additionalProperties": {
            "oneOf": [
              {
                "type": "string"
              },
              {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            ]
          }
        },
        "agda.goalLabels": {
          "type": "boolean",
          "default": true,
          "description": "Show goal ID labels (?0, ?1, ...) next to interaction points"
        },
        "agda.reloadOnGive": {
          "type": "boolean",
          "default": false,
          "description": "Automatically reload the file after give or refine actions"
        }
      }
    }
